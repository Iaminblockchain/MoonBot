{
    "$schema": "https://raw.githubusercontent.com/webpro/knip/main/schemas/v1.json",
    "entry": ["src/index.ts", "src/**/*.ts"],
    "project": ["**/*.ts"],
    "ignoreDependencies": ["ts-node"],
    "ignore": ["utilscripts/*.ts", "tests/*.ts"],
    "rules": {
        "exports": "error",
        "files": "error",
        "dependencies": "error",
        "classMembers": "warn"
    }
}
